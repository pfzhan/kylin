groups:
- name: app_status_alert
  rules:

  - alert: Instance Down
    expr: up{job=~"Kyligence Enterprise.*"} == 0
    for: 10m
    labels:
      severity: critical
    annotations:
      summary: "{{$labels.job}} Instance {{$labels.instance}} Down"
      description: "{{$labels.job}} Instance {{$labels.instance}} Down over 10 minutes"