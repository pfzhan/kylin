    <dependencies>
        <!--KAP-->
        <dependency>
            <groupId>io.kyligence.ke</groupId>
            <artifactId>kap-core-common</artifactId>
        </dependency>
        <dependency>
            <groupId>io.kyligence.ke</groupId>
            <artifactId>kap-core-metadata</artifactId>
        </dependency>
        <dependency>
            <groupId>io.kyligence.ke</groupId>
            <artifactId>kap-query</artifactId>
        </dependency>
        <dependency>
            <groupId>io.kyligence.ke</groupId>
            <artifactId>kap-spark-common</artifactId>
        </dependency>
        <dependency>
            <groupId>io.kyligence.ke</groupId>
            <artifactId>kap-second-storage-core</artifactId>
        </dependency>
        <dependency>
            <groupId>io.kyligence.ke</groupId>
