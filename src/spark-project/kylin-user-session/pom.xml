<project xmlns="http://maven.apache.org/POM/4.0.0"
         xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
         xsi:schemaLocation="http://maven.apache.org/POM/4.0.0 http://maven.apache.org/xsd/maven-4.0.0.xsd">
    <parent>
        <artifactId>ke</artifactId>
        <groupId>io.kyligence.ke</groupId>
        <version>4.5.9-SNAPSHOT</version>
        <relativePath>../../../pom.xml</relativePath>
    </parent>
    <modelVersion>4.0.0</modelVersion>

    <artifactId>kylin-user-session</artifactId>
    <packaging>jar</packaging>
    <name>KAP - KylinUserSession</name>
    <url>http://kyligence.io</url>
    <description>Kyligence Kylin User Session</description>


    <dependencies>
        <!--KAP-->
        <dependency>
            <groupId>io.kyligence.ke</groupId>
            <artifactId>kap-core-common</artifactId>
            <scope>provided</scope>
        </dependency>
        <dependency>
            <groupId>io.kyligence.ke</groupId>
            <artifactId>kap-spark-common</artifactId>
            <version>${project.version}</version>
            <scope>provided</scope>
        </dependency>
        <dependency>
            <groupId>io.kyligence.ke</groupId>
            <artifactId>kap-core-metadata</artifactId>
            <scope>provided</scope>
        </dependency>
        <dependency>
            <groupId>io.kyligence.ke</groupId>
            <artifactId>kap-sparder</artifactId>
            <version>${project.version}</version>
            <scope>provided</scope>
        </dependency>

        <dependency>
            <groupId>org.apache.commons</groupId>
            <artifactId>commons-dbcp2</artifactId>
            <version>2.5.0</version>
            <scope>provided</scope>
        </dependency>

    </dependencies>

    <build>
        <plugins>
            <plugin>
                <groupId>org.apache.maven.plugins</groupId>
                <artifactId>maven-shade-plugin</artifactId>

                <executions>
                    <execution>
                        <phase>package</phase>
                        <goals>
                            <goal>shade</goal>
                        </goals>
                        <configuration>
                            <filters>
                                <filter>
                                    <artifact>*:*</artifact>
                                    <excludes>
                                        <exclude>META-INF/*.SF</exclude>
                                        <exclude>META-INF/*.DSA</exclude>
                                        <exclude>META-INF/*.RSA</exclude>
                                    </excludes>
                                </filter>
                            </filters>
                        </configuration>
                    </execution>

                </executions>
            </plugin>
        </plugins>
    </build>

</project>