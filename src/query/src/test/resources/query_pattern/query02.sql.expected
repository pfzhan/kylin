SELECT "L_SHIPMODE", SUM((CASE WHEN (("O_ORDERPRIORITY" = '1-URGENT') OR ("O_ORDERPRIORITY" = '2-HIGH')) THEN 1 ELSE 0 END)) "HIGH_LINE_COUNT", SUM((CASE WHEN (("O_ORDERPRIORITY" <> '1-URGENT') AND ("O_ORDERPRIORITY" <> '2-HIGH')) THEN 1 ELSE 0 END)) "LOW_LINE_COUNT"
FROM "TPCH"."LINEITEM"
INNER JOIN "TPCH"."ORDERS" ON ("O_ORDERKEY" = "L_ORDERKEY")
WHERE ((((("L_SHIPMODE" IN ('A')) AND ("L_COMMITDATE" < "L_RECEIPTDATE")) AND ("L_SHIPDATE" < "L_COMMITDATE")) AND ("L_RECEIPTDATE" >= '2010-01-01')) AND ("L_RECEIPTDATE" < '2010-01-02'))
GROUP BY "L_SHIPMODE"
ORDER BY "L_SHIPMODE"